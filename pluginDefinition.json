{
  "name": "HTTP",
  "pluginId": "stedi-http",
  "description": "Stedi HTTP delivery plugin",
  "version": "0.2.0",
  "credentialsSchema": {
    "type": "object",
    "properties": {
      "headers": {
        "type": "object"
      }
    },
    "required": []
  },
  "model": [
    {
      "namespace": "http",
      "operations": {
        "deliver": {
          "implementedBy": {
            "type": "function",
            "functionName": "stedi-http-deliver-v1"
          },
          "input": {
            "parametersSchema": {
              "type": "object",
              "properties": {
                "url": {
                  "type": "string"
                },
                "method": {
                  "type": "string",
                  "enum": ["GET", "POST", "PUT", "DELETE", "PATCH"]
                },
                "headers": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "string"
                  }
                },
                "query": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "string"
                  }
                }
              },
              "required": ["method", "url"]
            }
          },
          "output": {
            "deduplication": {
              "enabled": false
            }
          }
        }
      }
    }
  ]
}
